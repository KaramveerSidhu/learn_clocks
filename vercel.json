{
  "version": 2,
  "builds": [
    {
      "src": "learn_clocks/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.5" }
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "learn_clocks/wsgi.py"
    }
  ],
  "functions": {
    "learn_clocks/wsgi.py": {
      "includeFiles": "db.sqlite3"
    }
  }
}
